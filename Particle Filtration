function zfishF = part_fil(zfishI)
for r = size(zfishI, 2)
  for c = size(zfishI, 1)
   if pixel <= 0.5 
     new_pixel = 0.00;   % sends the image to black
   elseif pixel => 3.0
     new_pixel = 256;    % sends the image to white
   else
     new_pixel = pixel
   end
   zfishF(r,c) = new_pixel; % sets each to position to its pixel
end
end
 
